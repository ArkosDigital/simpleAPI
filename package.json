{
  "name": "simple-api",
  "version": "1.0.0",
  "main": "server.js",
  "license": "MIT",
  "scripts": {
    "tsc": "tsc",
    "prestart": "tsc",
    "start": "node dist/src/server.js",
    "dev": "nodemon src/server.ts",
    "dev:debug": "nodemon --inspect src/server.ts",
    "build": "sucrase ./src -d ./dist --transforms typescript,imports",
    "test": "jest --passWithNoTests --ci --verbose --forceExit --detectOpenHandles",
    "integration": "jest -i -c integration.config.json --passWithNoTests  --ci --verbose --forceExit --detectOpenHandles"
  },
  "dependencies": {
    "bignumber.js": "^9.0.0",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "flexiblepersistence": "^1.0.52",
    "google-auth-library": "^5.10.1",
    "jsonwebtoken": "^8.5.1",
    "mongodb": "^3.5.5",
    "pg": "^7.18.2",
    "ts-mixer": "^5.1.0"
  },
  "devDependencies": {
    "@types/bignumber.js": "^5.0.0",
    "@types/cors": "^2.8.6",
    "@types/express": "^4.17.3",
    "@types/jest": "^25.1.4",
    "@types/node": "^13.9.0",
    "@types/pg": "^7.14.3",
    "@typescript-eslint/eslint-plugin": "^2.23.0",
    "@typescript-eslint/parser": "^2.23.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-eslint-snake-case": "^0.0.6",
    "eslint-plugin-filenames": "1.3.2",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-prettier": "^3.1.2",
    "jest": "^25.1.0",
    "nodemon": "^2.0.2",
    "prettier": "^1.19.1",
    "sucrase": "^3.12.1",
    "ts-jest": "^25.2.1",
    "typescript": "^3.8.3"
  }
}